        var editCustomConfig = JSON.parse($('#edit-CustomConfig').val());
        var tableLayout = editCustomConfig.TableLayout;
        $('#pliot-page3').find('input[name="PilotFormat"][value="' + tableLayout + '"]').attr('checked', true);
